r><td data-text-align="left" data-vertical-align="top">2-oktyl-isotiazol-3(2H)-on (OIT)</td><td data-text-align="center" data-vertical-align="top">26530-20-1</td><td data-text-align="center" data-vertical-align="top">8</td><td data-text-align="center" data-vertical-align="top">01.01.2018</td><td data-text-align="center" data-vertical-align="top">(EU) 2017/1277</td><td data-text-align="center" data-vertical-align="top">Nr. 12zzzzu</td></tr><tr><td data-text-align="left" data-vertical-align="top">2-metylisotiazol-3(2H)-on (MIT)</td><td data-text-align="center" data-vertical-align="top">2682-20-4</td><td data-text-align="center" data-vertical-align="top">11</td><td data-text-align="center" data-vertical-align="top">01.01.2019</td><td data-text-align="center" data-vertical-align="top">(EU) 2017/1278</td><td data-text-align="center" data-vertical-align="top">Nr. 12zzzzv</td></tr><tr><td data-text-align="left" data-vertical-align="top">Warfarin</td><td data-text-align="center" data-vertical-align="top">81-81-2</td><td data-text-align="center" data-vertical-align="top">14</td><td data-text-align="center" data-vertical-align="top">01.02.2010</td><td data-text-align="center" data-vertical-align="top">2010/11/EU<br/>(EU) 2017/1376</td><td data-text-align="center" data-vertical-align="top">Nr. 12n<br/>Nr. 12zzzzx</td></tr><tr><td data-text-align="left" data-vertical-align="top">Klorfacinon</td><td data-text-align="center" data-vertical-align="top">3691-35-8</td><td data-text-align="center" data-vertical-align="top">14</td><td data-text-align="center" data-vertical-align="top">01.07.2011</td><td data-text-align="center" data-vertical-align="top">2009/99/EF<br/>(EU) 2017/1377</td><td data-text-align="center" data-vertical-align="top">Nr. 12n<br/>Nr. 12zzzzy</td></tr><tr><td data-text-align="left" data-vertical-align="top">Kumatetralyl</td><td data-text-align="center" data-vertical-align="top">5836-29-3</td><td data-text-align="center" data-vertical-align="top">14</td><td data-te