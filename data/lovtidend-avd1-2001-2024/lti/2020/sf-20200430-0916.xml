                <td data-text-align="left" data-vertical-align="top">12 &#xE5;r</td>
              </tr>
              <tr>
                <td data-text-align="left" data-vertical-align="top">t.o.m. 4 &#xE5;r:</td>
                <td data-text-align="left" data-vertical-align="top">13,5 &#xE5;r</td>
              </tr>
              <tr>
                <td data-text-align="left" data-vertical-align="top">t.o.m. 5 &#xE5;r:</td>
                <td data-text-align="left" data-vertical-align="top">15 &#xE5;r</td>
              </tr>
              <tr>
                <td data-text-align="left" data-vertical-align="top">t.o.m. 6 &#xE5;r:</td>
                <td data-text-align="left" data-vertical-align="top">16,5 &#xE5;r</td>
              </tr>
